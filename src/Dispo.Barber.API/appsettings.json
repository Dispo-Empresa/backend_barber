{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Twilio": {
        "AccountSid": "${{ TWILIO_ACCOUNT_SID }}",
        "AuthToken": "${{ TWILIO_AUTH_TOKEN }}",
        "PhoneNumber": "${{ TWILIO_PHONE_NUMBER }}",
        "PhoneNumberWhats": "${{ TWILIO_PHONE_NUMBER_WHATS }}"
    }
}
